#! /usr/sbin/cfagent -f

control:
   any::
   actionsequence = ( files directories editfiles )
   EditFileSize = ( 30000 )

files:
   any::

directories:
   any::
	${target}/root/.ssh mode=0700 owner=0 group=0

editfiles:
   any::
	{ ${target}/root/.ssh/authorized_keys
	  AutoCreate
	  AppendIfNoSuchLine "ssh-rsa  holger@matrix"
	  AppendIfNoSuchLine "ssh-rsa  carl@dc10"
	  AppendIfNoSuchLine "ssh-rsa  jaylkam@unknownc8e0eb13e69b.att.net"
	  AppendIfNoSuchLine "ssh-rsa  wouter@celtic"
	  AppendIfNoSuchLine "ssh-rsa  formorer@nelson"
	}


